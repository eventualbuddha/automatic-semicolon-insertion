    "@types/babel__traverse": "^7.17.1",
    "@types/jest": "^28.1.0",
    "@types/node": "^14.18.20",
    "jest": "^28.1.0",
    "rollup": "^2.75.5",
    "ts-jest": "^28.0.4",
